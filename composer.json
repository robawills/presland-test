{
  "require": {
    "barrelstrength/sprout-forms": "3.0.0-beta.45",
    "craftcms/cms": "^3.0.0",
    "craftcms/redactor": "2.3.2",
    "ether/simplemap": "3.3.5",
    "vlucas/phpdotenv": "^2.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
